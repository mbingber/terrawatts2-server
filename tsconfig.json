{
   "compilerOptions": {
      "lib": [
         "dom",
         "es5",
         "es6",
         "esnext.asynciterable"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "rootDir": "./src",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true
   },
   "exclude": ["./node_modules"]
}