{
   "name": "terrawatts2-server",
   "version": "0.0.1",
   "description": "Terrawatts 2 GraphQL server",
   "engines": {
      "node": "12.13.1",
      "npm": "6.12.1"
   },
   "dependencies": {
      "@types/node": "^8.0.29",
      "apollo-server-express": "^2.12.0",
      "axios": "^0.19.0",
      "bcryptjs": "^2.4.3",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "graphql": "^14.5.8",
      "graphql-import": "^0.7.1",
      "graphql-redis-subscriptions": "^2.2.1",
      "ioredis": "^4.16.1",
      "iterall": "^1.3.0",
      "jsonwebtoken": "^8.5.1",
      "lodash.shuffle": "^4.2.0",
      "pg": "^7.3.0",
      "pg-connection-string": "^2.2.0",
      "reflect-metadata": "^0.1.10",
      "rimraf": "^3.0.2",
      "ts-node": "^3.3.0",
      "typeorm": "0.2.21",
      "typescript": "^3.3.3333"
   },
   "scripts": {
      "build": "rimraf ./build && tsc -p ./tsconfig.json",
      "postinstall": "yarn build",
      "start": "node ./build/index.js",
      "start:dev": "ts-node src/index.ts",
      "migrate": "ts-node ./node_modules/.bin/typeorm migration:run",
      "revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
      "heroku-postbuild": "yarn migrate"
   }
}
